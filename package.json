{
  "name": "crm-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-ui": "npx tsx ./codemod/migrate-jhipster-entities.ts --force",
    "generate-entities": "node ./scripts/generate/generate-entities.js",
    "generate-entity": "node ./scripts/generate/generate-entities.js --entity",
    "generate-entities:overwrite": "node ./scripts/generate/generate-entities.js --overwrite",
    "verify-directories": "node ./scripts/generate/verify-directories.js",
    "fix-directories": "node ./scripts/generate/verify-directories.js --fix",
    "verify-hooks": "node ./scripts/generate/verify-hooks.js",
    "prebuild": "npm run verify-directories && npm run verify-hooks",
    "generate-nextjs": "npx ts-node codgen/nextjs/generator.ts",
    "generate-nextjs:entity": "cd codgen/nextjs && npm run generate:entity",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:setup": "npm run db:generate && npm run db:migrate",
    "db:validate": "npx tsx scripts/validate-session-setup.ts",
    "db:cleanup": "npx tsx scripts/session-cleanup.ts",
    "db:seed": "npx tsx scripts/seed-database.ts",
    "auth:validate": "npm run db:validate",
    "auth:cleanup": "npm run db:cleanup"
  },
