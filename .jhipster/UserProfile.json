  "documentation": "Extended user profile linked to Keycloak.",
  "dto": "mapstruct",
  "fields": [
    {
      "fieldName": "keycloakId",
      "fieldType": "String",
      "fieldValidateRules": ["pattern"],
      "fieldValidateRulesPattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
