  "applications": ["crmBackend"],
  "documentation": "Keycloak Organization mapping",
  "dto": "mapstruct",
  "fields": [
    {
      "fieldName": "keycloakOrgId",
      "fieldType": "String",
      "fieldValidateRules": ["required", "pattern"],
      "fieldValidateRulesPattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
