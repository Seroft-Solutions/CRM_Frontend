    {
      "fieldName": "name",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "100",
      "fieldValidateRulesMinlength": "2"
    },
    {
      "fieldName": "displayName",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "150"
    },
    {
      "fieldName": "domain",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength", "pattern"],
      "fieldValidateRulesMaxlength": "100",
      "fieldValidateRulesPattern": "^[a-zA-Z0-9]([a-zA-Z0-9\\-]{0,61}[a-zA-Z0-9])?(\\.[a-zA-Z0-9]([a-zA-Z0-9\\-]{0,61}[a-zA-Z0-9])?)*\\.[a-zA-Z]{2,}$"
    },
    {
      "fieldName": "isActive",
      "fieldType": "Boolean",
      "fieldValidateRules": ["required"]
    },
    {
      "fieldName": "createdAt",
      "fieldType": "Instant"
    },
    {
      "fieldName": "updatedAt",
      "fieldType": "Instant"
    }
  ],
  "jpaMetamodelFiltering": true,
  "name": "Organization",
  "pagination": "pagination",
  "relationships": [
    {
