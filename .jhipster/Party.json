{
  "annotations": {
    "changelogDate": "20250525054424"
  },
  "applications": "*",
  "documentation": "Party entity - represents customers/leads/contacts",
  "dto": "mapstruct",
  "fields": [
    {
      "documentation": "Party name/company name",
      "fieldName": "name",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "100",
      "fieldValidateRulesMinlength": "2"
    },
    {
      "documentation": "Mobile number",
      "fieldName": "mobile",
      "fieldType": "String",
      "fieldValidateRules": ["required", "pattern"],
      "fieldValidateRulesPattern": "^[+]?[0-9]{10,15}$"
    },
    {
      "documentation": "Email address",
      "fieldName": "email",
      "fieldType": "String",
      "fieldValidateRules": ["pattern", "maxlength"],
      "fieldValidateRulesMaxlength": "254",
      "fieldValidateRulesPattern": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$"
    },
    {
      "documentation": "WhatsApp number",
      "fieldName": "whatsApp",
      "fieldType": "String",
      "fieldValidateRules": ["pattern"],
      "fieldValidateRulesPattern": "^[+]?[0-9]{10,15}$"
    },
    {
      "documentation": "Contact person name",
      "fieldName": "contactPerson",
      "fieldType": "String",
      "fieldValidateRules": ["minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "100",
      "fieldValidateRulesMinlength": "2"
    },
    {
      "documentation": "Address line 1",
      "fieldName": "address1",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "255"
    },
    {
      "documentation": "Address line 2",
      "fieldName": "address2",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "255"
    },
    {
      "documentation": "Address line 3",
      "fieldName": "address3",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "255"
    },
    {
      "documentation": "Website URL",
      "fieldName": "website",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength", "pattern"],
      "fieldValidateRulesMaxlength": "255",
      "fieldValidateRulesPattern": "^https?:\\/\\/[^\\s]+$"
    },
    {
      "documentation": "Company/individual type",
      "fieldName": "partyType",
      "fieldType": "String",
      "fieldValidateRules": ["required", "maxlength"],
      "fieldValidateRulesMaxlength": "20"
    },
    {
      "documentation": "Lead status",
      "fieldName": "leadStatus",
      "fieldType": "LeadStatus",
      "fieldTypeDocumentation": "Lead status enumeration",
      "fieldValidateRules": ["required"],
      "fieldValues": "NEW,CONTACTED,QUALIFIED,PROPOSAL_SENT,NEGOTIATION,CLOSED_WON,CLOSED_LOST,ON_HOLD"
    },
    {
      "documentation": "Lead score (0-100)",
      "fieldName": "leadScore",
      "fieldType": "Integer",
      "fieldValidateRules": ["min", "max"],
      "fieldValidateRulesMax": "100",
      "fieldValidateRulesMin": "0"
    },
    {
      "documentation": "Annual revenue",
      "fieldName": "annualRevenue",
      "fieldType": "BigDecimal",
      "fieldValidateRules": ["min"],
      "fieldValidateRulesMin": "0"
    },
    {
      "documentation": "Number of employees",
      "fieldName": "employeeCount",
      "fieldType": "Integer",
      "fieldValidateRules": ["min"],
      "fieldValidateRulesMin": "0"
    },
    {
      "documentation": "Is this party active",
      "fieldName": "isActive",
      "fieldType": "Boolean",
      "fieldValidateRules": ["required"]
    },
    {
      "documentation": "Registration date",
      "fieldName": "registrationDate",
      "fieldType": "LocalDate",
      "fieldValidateRules": ["required"]
    },
    {
      "documentation": "Last contact date",
      "fieldName": "lastContactDate",
      "fieldType": "LocalDate"
    },
    {
      "documentation": "Next follow up date",
      "fieldName": "nextFollowUpDate",
      "fieldType": "LocalDate"
    },
    {
      "documentation": "Additional remarks",
      "fieldName": "remark",
      "fieldType": "TextBlob"
    },
    {
      "documentation": "Created timestamp",
      "fieldName": "createdDate",
      "fieldType": "Instant",
      "fieldValidateRules": ["required"]
    },
    {
      "documentation": "Last modified timestamp",
      "fieldName": "lastModifiedDate",
      "fieldType": "Instant"
    }
  ],
  "jpaMetamodelFiltering": true,
  "name": "Party",
  "pagination": "pagination",
  "relationships": [
