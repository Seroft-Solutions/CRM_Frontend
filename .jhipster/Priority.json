{
  "annotations": {
    "changelogDate": "20250525054419"
  },
  "applications": "*",
  "documentation": "Priority master entity for calls and leads",
  "dto": "mapstruct",
  "fields": [
    {
      "documentation": "Priority name",
      "fieldName": "name",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "50",
      "fieldValidateRulesMinlength": "2"
    },
    {
      "documentation": "Priority level",
      "fieldName": "level",
      "fieldType": "PriorityLevel",
      "fieldTypeDocumentation": "Priority level enumeration",
      "fieldValidateRules": ["required"],
      "fieldValues": "LOW,MEDIUM,HIGH,URGENT,CRITICAL"
    },
    {
      "documentation": "Description of priority",
      "fieldName": "description",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "255"
    },
    {
      "documentation": "Color code for UI display",
      "fieldName": "colorCode",
      "fieldType": "String",
      "fieldValidateRules": ["pattern"],
      "fieldValidateRulesPattern": "^#[0-9A-Fa-f]{6}$"
    },
    {
      "documentation": "Sort order",
      "fieldName": "sortOrder",
      "fieldType": "Integer",
      "fieldValidateRules": ["min"],
      "fieldValidateRulesMin": "0"
    },
    {
      "documentation": "Additional remarks",
      "fieldName": "remark",
      "fieldType": "TextBlob"
    },
    {
      "documentation": "Active status",
      "fieldName": "isActive",
      "fieldType": "Boolean",
      "fieldValidateRules": ["required"]
    },
    {
      "documentation": "Created timestamp",
      "fieldName": "createdDate",
      "fieldType": "Instant",
      "fieldValidateRules": ["required"]
    },
    {
      "documentation": "Last modified timestamp",
      "fieldName": "lastModifiedDate",
      "fieldType": "Instant"
    }
  ],
  "jpaMetamodelFiltering": true,
  "name": "Priority",
  "pagination": "pagination",
  "relationships": [],
  "service": "serviceImpl"
}
